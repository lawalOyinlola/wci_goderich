{
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/church",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/ministry",
      "destination": "/service-units",
      "permanent": true
    },
    {
      "source": "/sermons",
      "destination": "/media",
      "permanent": true
    },
    {
      "source": "/news",
      "destination": "/events",
      "permanent": true
    },
    {
      "source": "/photos",
      "destination": "/gallery",
      "permanent": true
    },
    {
      "source": "/donate",
      "destination": "/giving",
      "permanent": true
    },
    {
      "source": "/volunteer",
      "destination": "/service-units",
      "permanent": true
    },
    {
      "source": "/cell-groups",
      "destination": "/homecell",
      "permanent": true
    },
    {
      "source": "/bible-school",
      "destination": "/wofbi",
      "permanent": true
    },
    {
      "source": "/pastor",
      "destination": "/pastors",
      "permanent": true
    },
    {
      "source": "/testimony",
      "destination": "/testimonies",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs"
}
